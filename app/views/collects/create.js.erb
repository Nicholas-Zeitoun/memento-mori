function createCollect() {
  swal({
    text: "meme collected!",
    icon: "success",
  });
  const imgForm = document.querySelector("[data-meme='<%= @collect.meme_id %>']")
  const myColl = document.querySelector(".js-ajax-mycollections")
  console.log("hey from collect ajax");
  imgForm.innerHTML = '<%= j render "/memes/meme_card_overlay", meme: Meme.find(params[:meme_id]) %>';
  myColl.innerHTML = '<%= j render "/shared/sidebars/left_sidebar_mycollect" %>';
}

createCollect();
